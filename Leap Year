import java.util.*;
class Main 
{
    public static void main(String[] args) 
    {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();

        boolean flag=false;
        
        if((n%4==0 && n%100!=0) || n%400==0)
        {
            flag=true;
        }

        System.out.println(flag ? "Leap Year" : "Not Leap year");
    }
}
