import java.util.*;

class Main
{
    public static void main(String[] args)
    {
        Scanner in=new Scanner(System.in);
        
        int n=in.nextInt(); //Size of the Array
        
        int a[]=new int[n]; //Array Initialization
        
        for(int i=0;i<n;i++)
        {
            a[i]=in.nextInt(); //Input of Array Elements
        }
        
        int sum=0;
        for(int i=0;i<n;i++)
        {
            sum+=a[i]; //Adding array Elements one by one
        }
        
        System.out.println(sum);
    }
}


